// Órdenes esperadas
const ordenes = consultaOrdenes.map(o =>
  o['3_NumeroOrdenContenedor']?.trim()
);

// Ingresos registrados
const ingresosMap = new Map(
  consultaIngreso.map(i => [
    i['3_NumOrden_Verificacion']?.trim(),
    i['3_Ingreso_Verificacion']
  ])
);

// Estado por defecto
let estado = 1;

// Validar todas las órdenes
for (const orden of ordenes) {
  const ingreso = ingresosMap.get(orden);

  // Si no existe la orden o el ingreso está vacío → estado 0
  if (!ingreso || String(ingreso).trim() === '') {
    estado = 0;
    break;
  }
}


