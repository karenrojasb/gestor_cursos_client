import { Controller, Post, Body, BadRequestException, HttpCode, HttpStatus } from '@nestjs/common';
import { generarOrdenService } from './generarorden.service';
import { GenerarOrdenDto } from './dto/generarorden.dto';


@Controller()
export class generarOrdenController{ 
    constructor(private readonly generarOrdenService: generarOrdenService) {}

    @Post('generarorden')
    @HttpCode(HttpStatus.OK)
    async generarOrden(@Body() data: GenerarOrdenDto){
    return this.generarOrdenService.GenerarOrden(data);
  } 

}

POST http://localhost:3000/generarorden

