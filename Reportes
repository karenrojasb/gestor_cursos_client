if (resultHotel.length > 0 && resultHotel[0].numeroCDP) {
    const numeroFormateado = padLeftZeros(resultHotel[0].numeroCDP);
    await ejecutarAnulacion(numeroFormateado, resultHotel[0].valor);
}




