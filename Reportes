SELECT 
    I.ID,
    I.[3_codigo] + ' - ' + I.[3_NArticulo] AS codItem,
    I.[3_Cantidad] AS CantidadT,
    ISNULL(I.[3_cantidadingr], 0) AS CantidadI,
    ISNULL(I.[3_cantidadreciart], 0) AS CantidadR,
    ISNULL(I.[3_numfacturaart], '') AS factura
FROM Panel_109_68508_68526 I
INNER JOIN Panel_109_68508 P ON P.ID = I._ElementID
INNER JOIN AP__BPM_Procesos PR ON PR.ID = P._ElementID
WHERE PR.Referencia = 'SCM-38.3_18587'
ORDER BY I.ID;