if (tipoIngreso.toLowerCase() === 'parcial') {
  if (totalIngresosGenerados === 0) {
    return {
      Codigo: '200',
      Message: '0 ingresos no est√°n completos',
      totalIngresosGenerados
    };
  }

  return {
    Codigo: '200',
    Message: `Se generaron ${totalIngresosGenerados} ingresos`,
    totalIngresosGenerados,
    asig_numero
  };
}


