let estadoIngreso: number | null = null;

if (tipoIngreso.toLowerCase() === 'parcial') {
  estadoIngreso = totalIngresosGenerados === 0 ? 0 : 1;
}





const Message =
  (tipoIngreso.toLowerCase() === 'parcial'
    ? totalIngresosGenerados === 0
      ? 'Los ingresos no están completos. '
      : 'Ya hay ingresos generados. '
    : '') + 'Consulta realizada con éxito';