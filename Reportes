 let cod_sub = '';
    if (asigNumero.startsWith('OS') || asigNumero.startsWith('OC')) {
      cod_sub = 'C008';
    } else if (asigNumero.startsWith('PF')) {
      cod_sub = 'PFIN';
    } else if (asigNumero.startsWith('CN')) {
      cod_sub = 'NCIN';
    } else {
      throw new Error(`No se pudo determinar el sub_tip para asig_numero ${asigNumero}`);
    }
