await this.prismaNovasoftService.$executeRawUnsafe(`
    EXEC dbo.USR_sp_gen_afeinv_preciso
        @iano_doc = @iano_doc,
        @fano_doc = @fano_doc,
        @iper_doc = @iper_doc,
        @fper_doc = @fper_doc,
        @isub_tip = @isub_tip,
        @fsub_tip = @fsub_tip,
        @inum_doc = @inum_doc,
        @fnum_doc = @fnum_doc,
        @ifec_doc = @ifec_doc,
        @ffec_doc = @ffec_doc,
        @indver = 0
`, 
getNowDate().getFullYear(),          // @iano_doc
getNowDate().getFullYear(),          // @fano_doc
getCurrentMonth(),                    // @iper_doc
getCurrentMonth(),                    // @fper_doc
cod_sub,                              // @isub_tip
cod_sub,                              // @fsub_tip
numDocConsecutivo,                    // @inum_doc
numDocConsecutivo,                    // @fnum_doc
formatoDateToDDMMYYYY(),              // @ifec_doc
formatoDateToDDMMYYYY()               // @ffec_doc
);