
let mensaje = 'Consulta realizada con éxito';

if (tipoIngreso.toLowerCase() === 'parcial') {
  if (totalIngresosGenerados === 0) {
    mensaje = 'Los ingresos no están completos. ' + mensaje;
  } else {
    mensaje = 'Ya hay ingresos generados. ' + mensaje;
  }
}




return {
  Codigo: '200',
  Message: mensaje,
  asig_numero
};


const Message =
  (tipoIngreso.toLowerCase() === 'parcial'
    ? totalIngresosGenerados === 0
      ? 'Los ingresos no están completos. '
      : 'Ya hay ingresos generados. '
    : '') + 'Consulta realizada con éxito';
