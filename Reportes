const cantidad1 = consulta1.length > 0
  ? Number(consulta1[0].cantidad)
  : null;

const cantidad2 = consulta2.length > 0
  ? Number(consulta2[0].cantidad)
  : null;



let estado = 0;

// 1️⃣ Si la consulta 1 no trae datos
if (cantidad1 === null) {
  estado = 0;
}
// 2️⃣ Si ambas cantidades existen y son iguales
else if (cantidad2 !== null && cantidad1 === cantidad2) {
  estado = 1;
}
// 3️⃣ Si cantidad 1 es menor que cantidad 2
else if (cantidad2 !== null && cantidad1 < cantidad2) {
  estado = 0;
}



