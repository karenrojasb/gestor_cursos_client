generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

generator clientAura {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

datasource dbAura {
  provider = "sqlserver"
  url      = env("AURAQUANTIC_URL")
}



